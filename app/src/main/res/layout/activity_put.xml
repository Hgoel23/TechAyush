<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.kalya.harsh.Put">
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Put">
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Symptoms"
    android:textSize="24dp"
    android:layout_margin="20dp"
    android:textColor="@color/black"
    android:id="@+id/symtoms"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cold"
        android:id="@+id/cold"
        android:layout_margin="10dp"
        android:textSize="18dp"
        android:layout_below="@+id/symtoms"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="fever"
        android:id="@+id/fever"
        android:layout_margin="10dp"
        android:textSize="18dp"
        android:layout_toRightOf="@+id/cold"
        android:layout_below="@+id/symtoms"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="vomiting"
        android:id="@+id/HeartAttack"
        android:layout_margin="10dp"
        android:textSize="18dp"
        android:layout_toRightOf="@+id/fever"
        android:layout_below="@+id/symtoms"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Diarrhea"
        android:id="@+id/stroke"
        android:layout_margin="10dp"
        android:textSize="18dp"
        android:layout_below="@+id/cold"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fatigue"
        android:id="@+id/reproductivehealth"
        android:layout_margin="10dp"
        android:textSize="18dp"
        android:layout_toRightOf="@+id/stroke"
        android:layout_below="@+id/cold"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Muscle aches"
        android:id="@+id/breastproblems"
        android:layout_margin="10dp"
        android:textSize="18dp"
        android:layout_below="@+id/stroke"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Coughing"
        android:id="@+id/lungproblems"
        android:layout_margin="10dp"
        android:layout_toRightOf="@+id/breastproblems"
        android:textSize="18dp"
        android:layout_below="@+id/stroke"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="digestive problems"
        android:id="@+id/digestiveproblems"
        android:layout_margin="10dp"
        android:textSize="18dp"
        android:layout_below="@+id/breastproblems"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="skin problems"
        android:id="@+id/skinproblems"
        android:layout_margin="10dp"
        android:layout_toRightOf="@+id/digestiveproblems"
        android:textSize="18dp"
        android:layout_below="@+id/breastproblems"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" muscle or joint"
        android:id="@+id/muscleorjoint"
        android:layout_margin="10dp"
        android:textSize="18dp"
        android:layout_below="@+id/digestiveproblems"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="headache problem"
        android:id="@+id/headacheproblems"
        android:layout_margin="10dp"
        android:layout_toRightOf="@+id/muscleorjoint"
        android:textSize="18dp"
        android:layout_below="@+id/digestiveproblems"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="weight problems"
        android:id="@+id/weightproblems"
        android:layout_margin="10dp"
        android:textSize="18dp"
        android:layout_below="@+id/muscleorjoint"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Any Other Symptoms"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:layout_below="@+id/weightproblems"
        android:layout_margin="10dp"
        android:id="@+id/AnySymptons"/>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/AnySymptons"
        android:layout_margin="10dp"
        android:id="@+id/textfield123"/>
    <RelativeLayout
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:id="@+id/ok"
        android:layout_below="@+id/textfield123"
        android:layout_centerHorizontal="true">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/tick"/>
    </RelativeLayout>
</RelativeLayout>
</ScrollView>